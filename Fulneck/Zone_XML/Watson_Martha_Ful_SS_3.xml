                <zone points="1382,1759 1429,1760 1477,1761 1524,1761 1572,1761 1620,1760 1667,1759 1715,1758 1762,1756 1810,1755 1858,1753 1905,1751 1953,1749 2000,1747 2048,1745 2096,1743 2143,1740 2191,1738 2238,1737 2286,1734 2334,1733 2334,1693 2286,1694 2238,1697 2191,1698 2143,1700 2096,1703 2048,1705 2000,1707 1953,1709 1905,1711 1858,1713 1810,1715 1762,1716 1715,1718 1667,1719 1620,1720 1572,1721 1524,1721 1477,1721 1429,1720 1382,1719" rendition="Line" xml:id="facs_2_r2l28"/>
                <zone points="1382,1823 1425,1827 1469,1829 1513,1831 1557,1832 1601,1832 1645,1831 1689,1830 1733,1828 1777,1826 1821,1823 1864,1820 1908,1818 1952,1815 1996,1812 2040,1809 2084,1807 2128,1804 2172,1803 2216,1802 2260,1801 2260,1761 2216,1762 2172,1763 2128,1764 2084,1767 2040,1769 1996,1772 1952,1775 1908,1778 1864,1780 1821,1783 1777,1786 1733,1788 1689,1790 1645,1791 1601,1792 1557,1792 1513,1791 1469,1789 1425,1787 1382,1783" rendition="Line" xml:id="facs_2_r2l30"/>
